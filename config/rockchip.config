CONFIG_PACKAGE_kmod-i2c-smbus=y
CONFIG_PACKAGE_kmod-i2c-tiny-usb=y

# LuCI App
CONFIG_PACKAGE_luci-app-airplay2=y
CONFIG_PACKAGE_luci-app-filetransfer=y
CONFIG_PACKAGE_luci-app-oled=y
